<Defs>
    <StatDef ParentName="EBSG_FactoringStat">
        <defName>EBSG_DamageFactor_Humanlike</defName>
        <label>damage factor (humanlike)</label>
        <description>A multiplier on damage against humanlike targets.</description>
        <category>Basics</category>
        <displayPriorityInCategory>5202</displayPriorityInCategory>
        <modExtensions>
            <li Class="EBSGFramework.EBSGDamageExtension">
                <allowHumanlikes>True</allowHumanlikes>
            </li>
        </modExtensions>
    </StatDef>

